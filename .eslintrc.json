
{
    "extends": ["airbnb-base", "jquery"],
    "env": {
       "es6": true,
       "browser": true,
       "jest": true,
       "commonjs": true,
        "jquery": true
    },
    "rules": {
      "no-restricted-syntax": [
        "error",
        "LabeledStatement",
        "WithStatement"
